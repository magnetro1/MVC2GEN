  <LuaScript>--Run NOP Code Y &amp; Z
tempNOPY = getAddressList().getMemoryRecordByDescription("Y_NOP_Temp_Toggle")
tempNOPZ = getAddressList().getMemoryRecordByDescription("Z_NOP_Temp_Toggle")
tempNOPY.Active = true
tempNOPZ.Active = true
camPtr = {
--fov  = "[pcsx2.exe+026983F4]+FAC",
--xPos = "[pcsx2.exe+026983F4]+F4C",
--xRot = "[pcsx2.exe+026983F4]+F94",
yPos = "[pcsx2.exe+026983F4]+F50",
yRot = "[pcsx2.exe+026983F4]+F98",
zNOP = "[pcsx2.exe+026983F4]+FF4",
zSpt = "[pcsx2.exe+026983F8]",
--zPos = "[pcsx2.exe+026983F4]+F54",
--ZSpt = "[pcsx2.exe+026983F4]+F60",
}
-- Functions
-- Linear Interpolation
function lerp(v0, v1, t)
	return (1 - t) * v0 + t * v1
end
--Read Total Frame Counter
function readTF()
    local tfPointer = "pcsx2.exe+271A324"
    return readInteger(tfPointer)
end
-- Pans the camera over some amount of frames.
function cameraPan(cam, frames)
	assert(frames &gt; 0)
	local timeSpeed = 1 / frames
	local camStart = {}
	for i, v in pairs(camPtr) do
		assert(cam[i])
		camStart[i] = readFloat(v) -- Read float table
	end
	local currentTime = 0
	local oldFrame = readTF() -- Update Frame
	while currentTime &lt; 1.0 and contScript do	--Begin Writer
		currentFrame = readTF()
		while currentFrame == oldFrame and contScript do -- Break with Home key
			if isKeyPressed(VK_HOME) then
				contScript = false
			end
			currentFrame = readTF()
		end
		oldFrame = currentFrame
		currentTime = currentTime + timeSpeed -- Addition of time &amp; Movement
		for i, v in pairs(camPtr) do
			writeFloat(v, lerp(camStart[i], cam[i], currentTime))
		end
	end
	for i, v in pairs(camPtr) do
		writeFloat(v, cam[i])
	end
end
-- Script
